<Window x:Class="TOSOT_Praktika.MessageBoxInsert"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TOSOT_Praktika"
        mc:Ignorable="d"
        Title="MessageBoxInsert" Height="150" Width="200" WindowStyle="None" WindowStartupLocation="CenterScreen" MouseDown="Window_MouseDown" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="33*"/>
            <RowDefinition Height="119*"/>
            <RowDefinition Height="10*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="#FFDADAAB" Margin="0,7,0,0" ></Grid>
        <Grid Grid.Row="2" Background="#FFDADAAB" ></Grid>
        <Grid Grid.Row="1">
            <Label Content="Добавлено в список!" Style="{StaticResource lable}" Margin="25,30,17,41" FontSize="16"></Label>
            <Button  x:Name="insertfirm" Content="ОК" Style="{StaticResource MyButtonStyle}" Margin="120,74,10,10"  Width="Auto" Height="Auto" Click="insertfirm_Click"></Button>
        </Grid>
        
    </Grid>
</Window>
