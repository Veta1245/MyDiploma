<Window x:Class="TOSOT_Praktika.MessageBoxSuccessRegistration"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TOSOT_Praktika"
        mc:Ignorable="d"
        Title="MessageBoxSuccessRegistration" Height="150" Width="229.333" WindowStyle="None" WindowStartupLocation="CenterScreen" MouseDown="Window_MouseDown">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="33*"/>
            <RowDefinition Height="119*"/>
            <RowDefinition Height="10*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="#FFDADAAB" ></Grid>
        <Grid Grid.Row="2" Background="#FFDADAAB" ></Grid>
        <Grid Grid.Row="1">
            <Label Content="Регистрация прошла успешно!" Style="{StaticResource lable}" Margin="5,21,-10,50" FontSize="16"></Label>
            <Button  x:Name="SuccessRegistration" Content="ОК" Style="{StaticResource MyButtonStyle}" Margin="127,66,22,10"  Width="Auto" Height="Auto" Click="SuccessRegistration_Click"></Button>
        </Grid>

    </Grid>
</Window>
